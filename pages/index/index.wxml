<view class='index'>
  <swiper indicator-dots="true" autoplay="true" interval="3000" duration="500" circular="true" indicator-active-color="#8CDFF9" class='index-swiper'>
    <block wx:for="{{imgUrls}}" wx:key="item">
      <swiper-item>
        <image src="{{item}}" class="slide-image" />
      </swiper-item>
    </block>
  </swiper>
  <view class='index-tab'>
    <view class='intab-row'>
      <view class='tabrow-con' bindtap='toproperty'>
        <view class='tabrow-top'>
          <view class='rowcon-img'>
            <image src='/image/manageservice.png' class='allimg'></image>
          </view>
        </view>
        <view class='rowcon-text'>
          物业报修
        </view>
      </view>
      <view class='tabrow-con' bindtap='toit'>
        <view class='tabrow-top'>
          <view class='rowcon-imga'>
            <image src='/image/broadband.png' class='allimg'></image>
          </view>
        </view>
        <view class='rowcon-text'>
          宽带报修
        </view>
      </view>
      <view class='tabrow-con' bindtap='tograde'>
        <view class='tabrow-top'>
          <view class='rowcon-imgb'>
            <image src='/image/grade.png' class='allimg'></image>
          </view>
        </view>
        <view class='rowcon-text'>
          查询成绩
        </view>
      </view>
    </view>
    <view class='intab-row'>
      <view class='tabrow-con' bindtap='toclass'>
        <view class='tabrow-top'>
          <view class='rowcon-imgb'>
            <image src='/image/class.png' class='allimg'></image>
          </view>
        </view>
        <view class='rowcon-text'>
          课程信息
        </view>
      </view>
      <navigator class='tabrow-con' url='/pages/emptyroom/emptyroom' open-type='switchTab' hover-class="none">
        <view class='tabrow-top'>
          <view class='rowcon-img'>
            <image src='/image/findroom.png' class='allimg'></image>
          </view>
        </view>
        <view class='rowcon-text'>
          空教室查询
        </view>
      </navigator>
      <view class='tabrow-con' bindtap='toelewater'>
        <view class='tabrow-top'>
          <view class='rowcon-imgb'>
            <image src='/image/water.png' class='allimg'></image>
          </view>
        </view>
        <view class='rowcon-text'>
          水电缴费查询
        </view>
      </view>
    </view>
    <view class='intab-row'>
      <view class='tabrow-con' bindtap='tocourier'>
        <view class='tabrow-top'>
          <view class='rowcon-imgd'>
            <image src='/image/courier.png' class='allimg'></image>
          </view>
        </view>
        <view class='rowcon-text'>
          快递查询
        </view>
      </view>
      <view class='tabrow-con' bindtap='tofindbook'>
        <view class='tabrow-top'>
          <view class='rowcon-imgb'>
            <image src='/image/findbook.png' class='allimg'></image>
          </view>
        </view>
        <view class='rowcon-text'>
          图书馆找书
        </view>
      </view>
      <view class='tabrow-con' bindtap='toborrowinfo'>
        <view class='tabrow-top'>
          <view class='rowcon-imgb'>
            <image src='/image/borrow.png' class='allimg'></image>
          </view>
        </view>
        <view class='rowcon-text'>
          借阅信息
        </view>
      </view>
    </view>
  </view>
</view>